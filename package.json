{
  "name": "chill-dashboard",
  "version": "1.0.0",
  "description": "Chill dashboard - The monitoring tool.",
  "scripts": {
    "lint": "eslint src test; exit 0",
    "lint:fix": "eslint src test --fix; exit 0",
    "start:dev": "nodemon --watch .env --watch src/server --exec babel-node src/server --source-maps",
    "test": "NODE_ENV=test mocha",
    "test:watch": "NODE_ENV=test mocha --watch",
    "test:coverage": "NODE_ENV=test istanbul cover node_modules/mocha/bin/_mocha -- --recursive --require babel-register --colors --reporter spec test"
  },
  "keywords": [
    "chill",
    "dashboard"
  ],
  "private": false,
  "author": [],
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.2",
    "bookshelf-camelcase": "^1.1.4",
    "boom": "^4.2.0",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "helmet": "^3.2.0",
    "http-status-codes": "^1.0.6",
    "joi": "^10.1.0",
    "knex": "^0.12.6",
    "morgan": "^1.7.0",
    "serve-favicon": "^2.4.2",
    "sqlite3": "^3.1.8",
    "swagger-jsdoc": "^1.9.0",
    "winston": "^2.3.0",
    "winston-daily-rotate-file": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "codecov": "^2.1.0",
    "eslint": "^3.12.2",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.5.4",
    "supertest": "^2.0.1"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.10.8"
  }
}
